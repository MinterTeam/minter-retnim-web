{
  "name": "minter-retnim-web",
  "version": "1.0.0",
  "description": "Minter Download server",
  "author": "shrpne <shrpne@gmail.com>",
  "private": true,
  "scripts": {
    "prod": "npm run production",
    "production": "npm run build",
    "dev": "gulp & nuxt --spa --port=3015",
    "build": "gulp once && nuxt build --spa",
    "start": "nuxt start --spa --port=3003",
    "generate": "gulp once && nuxt generate",
    "analyze": "gulp once && nuxt build --spa --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "precommit": "echo 'Pre-commit checks...' && npm run lint"
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "custom-event-polyfill": "^1.0.7",
    "date-fns": "^2.0.0-alpha.27",
    "entity-decode": "^2.0.2",
    "fuse.js": "^3.4.4",
    "lodash-es": "^4.17.11",
    "nuxt": "^2.6.2",
    "pretty-bytes": "^5.1.0",
    "pretty-num": "0.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "beeper": "^2.0.0",
    "cross-env": "^5.2.0",
    "del": "^4.1.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.1",
    "gulp-cache": "^1.1.1",
    "gulp-clean-css": "^4.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "imagemin-mozjpeg": "^8.0.0",
    "postcss-normalize": "^7.0.1",
    "pre-commit": "^1.2.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
